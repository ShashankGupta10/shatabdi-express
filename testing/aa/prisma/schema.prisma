
        // schema.prisma

        generator client {
            provider = "prisma-client-js"
        }

        datasource db {
            provider = "sqlite"
            url      = env("DATABASE_URL")
        }

        // Example model
        model User {
            id    Int     @id @default(autoincrement())
            email       String  @unique
            name        String
            password    String
        }
    